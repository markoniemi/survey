<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:jaxws="http://cxf.apache.org/jaxws"
	xsi:schemaLocation="
	http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd 
	http://cxf.apache.org/jaxws http://cxf.apache.org/schemas/jaxws.xsd">
	<import resource="classpath:META-INF/cxf/cxf.xml" />
	<import resource="classpath:META-INF/cxf/cxf-servlet.xml" />
	<!-- Spring manage ServiceBean -->
	<!-- bean id must not clash with jaxws bean id -->
	<bean id="userServiceBean" class="org.survey.user.service.UserServiceImpl" />
	<!-- JAX-WS Service Endpoint -->
	<jaxws:endpoint id="userService" implementor="#userServiceBean"
		address="/soap/userService" />
</beans>